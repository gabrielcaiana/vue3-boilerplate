    "eslint": "^8.37.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "postcss": "^8.4.21",
    "simple-git-hooks": "^2.8.1",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
